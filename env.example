# HTTPS設定
HTTPS_ENABLED=true

# Azure Speech Service設定
AZURE_SPEECH_KEY=your_speech_key_here
AZURE_SPEECH_REGION=japaneast
AZURE_SPEECH_ENDPOINT=

# Garoon連携設定
GAROON_URL=https://jbccdemo.cybozu.com/g/cabinet/index.csp?sp=0&hid=7
GAROON_USERNAME=your_garoon_username
GAROON_PASSWORD=your_garoon_password

# iframe埋め込み設定
# iframe埋め込みを許可する場合はtrueに設定
ALLOW_IFRAME_EMBEDDING=false
# iframe埋め込みを許可するドメインを指定（*ですべて許可、特定ドメインを指定も可能）
ALLOWED_FRAME_ANCESTORS=*

# その他の環境変数...


